   "source": [
    "import pandas as pd\n",
    "\n",
    "student_dict = {'Name': ['Kate', 'Harry', 'Sheila'], 'Age': [10, 14, 12], 'Marks': [85, 77, 91]}\n",
    "\n",
    "# create DataFrame from dict\n",
    "df = pd.DataFrame(student_dict)\n",
    "print(df)\n",
    "\n",
    "gk = df.groupby('Age')\n",
    "gk.first()"
   ]
