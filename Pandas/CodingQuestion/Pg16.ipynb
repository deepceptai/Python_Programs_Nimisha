     "text": [
      "        x     y\n",
      "sum  1430  1864\n",
      "       x    y\n",
      "min  240  300\n",
      "       x     y\n",
      "max  630  1112\n"
     ]
    }
   ],
   "source": [
    "import pandas as pd\n",
    "\n",
    "data = {\n",
    "  \"x\": [560, 240, 630],\n",
    "  \"y\": [300, 1112, 452]\n",
    "}\n",
    "\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "x = df.aggregate([\"sum\"])\n",
    "y = df.aggregate([\"min\"])\n",
    "z = df.aggregate([\"max\"])\n",
    "\n",
    "print(x)\n",
    "print(y)\n",
    "print(z)"
   ]
  }
